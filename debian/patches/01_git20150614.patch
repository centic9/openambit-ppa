+        case ambit_log_sample_periodic_type_snr:
+        {
+            QString snr = QString("%02x%02x%02x%02x%02x%02x%02x%02x%02x%02x%02x%02x%02x%02x%02x%02x")
+                                           .arg(value->u.snr[0])
+                                           .arg(value->u.snr[1])
