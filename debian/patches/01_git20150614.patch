+void MovesCount::exit()
+{
+    static QMutex mutex;
+
+    exiting = true;
+
+    mutex.lock();
+    if (m_Instance) {
+        workerThread.quit();
+        workerThread.wait();
+
+        delete logChecker;
+
+        m_Instance = NULL;
+    }
+    mutex.unlock();
