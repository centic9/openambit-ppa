+        retEntry = new LogEntry();
+        QFile logfile(path);
+        logfile.open(QIODevice::ReadOnly);
+        XMLReader reader(retEntry);
+        if (!reader.read(&logfile)) {
+            QString error = reader.errorString();
+            qDebug() << "Failed to read " << path << ": " << error;
+            delete retEntry;
+            retEntry = NULL;
