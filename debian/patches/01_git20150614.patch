+QDateTime MovesCountJSON::dateTimeCompensate(QDateTime dateTime, QDateTime prevDateTime, int minOffset)
+{
+    if (dateTime <= prevDateTime) {
+        return prevDateTime.addMSecs(minOffset);
+    }
+    return dateTime;
+}
