+QString MovesCountXML::XMLWriter::dateTimeString(QDateTime &dateTime)
+{
+    if (dateTime.time().msec() != 0) {
+        return dateTime.toString("yyyy-MM-ddThh:mm:ss.zzzZ");
+    }
+    else {
+        return dateTime.toString("yyyy-MM-ddThh:mm:ssZ");
+    }
+}
