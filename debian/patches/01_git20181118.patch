+static float ieee754_to_float(uint32_t bits)
+{
+    int sign = bits >> 31 ? -1 : 1;
+    int exp = (int)(((bits >> 23) & 0xff) - 127);
+    int frac = (int)((bits & 0x7fffff) | 0x800000);
+
+    return sign * frac * powf(2.0f, exp - 23);
+}
